<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean class="com.hdv.api.ApplicationService" init-method="startup"
          destroy-method="shutdown" name="AppService">
			<property name="handlers">
				<map>
					<entry key="v1/GetDemo">
						<bean class="com.hdv.api.handlers.GetDemoHandler" />
					</entry>
					<entry key="v1/GetAllRole">
						<bean class="com.hdv.api.handlers.read.GetAllRoleHandler" />
					</entry>
					<entry key="v1/PostAccount">
						<bean class="com.hdv.api.handlers.write.PostAccountHandler" />
					</entry>
				</map>
			</property>
	</bean>
</beans>